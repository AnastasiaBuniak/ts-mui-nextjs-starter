{
    "name": "mui-nextjs-starter",
    "version": "0.1.1",
    "sideEffects": false,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "export": "next export",
        "prettier": "prettier --write src"
    },
    "dependencies": {
        "@emotion/cache": "^11.14.0",
        "@emotion/react": "^11.14.0",
        "@emotion/server": "^11.11.0",
