        "@mui/icons-material": "^6.2.0",
        "@mui/material": "^6.2.0",
        "@mui/x-date-pickers": "^7.25.0",
        "@next/bundle-analyzer": "^15.1.7",
        "dayjs": "^1.11.13",
        "front-matter": "^4.0.2",
        "glob": "^10.4.2",
        "js-yaml": "^4.1.0",
        "markdown-to-jsx": "^7.7.1",
        "next": "^15.1.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@stackbit/cms-git": "^1.0.2",
        "@stackbit/types": "^2.0.2",
        "@types/glob": "^8.1.0",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.7.4",
        "@types/react": "^19.0.1",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-config-next": "^15.1.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.3",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.3",
        "prettier": "^3.4.2",
        "typescript": "^5.4.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "**/*.{css,scss,md,json}": [
            "prettier --write",
            "git add"
        ]
    },
    "description": "A Nextjs page builder, component library and data source mapper all in one.",
    "main": "next.config.js",
    "license": "MIT"
}
